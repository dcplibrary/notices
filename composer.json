{
  "name": "dcplibrary/notices",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0||^10.0||^11.0||^12.0",
    "blashbrook/papiclient": "^2.0"
  },
  "type": "library",
  "description": "Laravel package for notices",
  "keywords": [
    "laravel",
    "package",
    "papi",
    "account"
  ],
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^10.0||^11.0",
    "orchestra/testbench": "^9.0||^10.0",
    "mockery/mockery": "^1.6",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.0",
    "larastan/larastan": "^2.0"
  },
  "authors": [
    {
      "name": "Brian Lashbrook",
      "email": "blashbrook@dcplibrary.org"
    }
  ],
  "homepage": "https://www.dcplibrary.org",
  "autoload": {
    "psr-4": {
      "Dcplibrary\\notices\\": "src/",
      "Dcplibrary\\notices\\App\\": "src/app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dcplibrary\\notices\\Tests\\": "src/tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "laravel": {
      "providers": [
        "Dcplibrary\\notices\\App\\Providers\\noticesServiceProvider"
      ],
      "aliases": {
        "notices": "Dcplibrary\\notices\\Facades\\notices"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "php-cs-fixer fix",
    "analyse": "phpstan analyse"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
