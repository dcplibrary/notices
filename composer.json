{
    "name": "dcplibrary/notices",
    "description": "Laravel package for tracking and analyzing Polaris ILS patron notices with Shoutbomb integration",
    "type": "library",
    "keywords": ["laravel", "polaris", "ils", "notices", "shoutbomb", "library"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Lashbrook",
            "email": "blashbrook@dcplibrary.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "livewire/livewire": "^3.7"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0|^11.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "friendsofphp/php-cs-fixer": "^3.90",
        "laravel/pint": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Dcplibrary\\Notices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dcplibrary\\Notices\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcplibrary\\Notices\\NoticesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
